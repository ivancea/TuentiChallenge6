ix.decode:
	GET #1, pc + 1
	o% = ASC(a$)
	PRINT ",";:print using "###"; o%;
	if opt%=1 then PRINT #2,",";:print #2,using "###";o%;
	if o%=203 then goto ixcb.decode
	RESTORE 4000
	FOR f%=0 TO o%: READ d$: NEXT f%
	pc = pc + 1
	if instr(d$,"IDS") then
		GET #1, pc + 1: edisp = ASC(a$)
		IF edisp > 127 THEN edisp = edisp - 256
		PRINT ",";:print using "###"; ASC(a$);
		if opt%=1 then PRINT #2,",";:print #2,using "###";ASC(a$);
		t$=mid$(d$,1,instr(d$,"IDS")-1)
		ed$=str$(edisp)
		if edisp>0 then ed$="+"+mid$(str$(edisp),2,3)
		if edisp=0 then ed$=""
		t$=t$+ed$
		t$=t$+mid$(d$,instr(d$,"IDS") + 3, 10)
		d$=t$
		pc = pc + 1
		end if
	IF INSTR(d$, "NN") THEN GOTO embword0
	IF INSTR(d$, "N") THEN GOTO embbyte0
	goto prdis
        GOTO disasm.more

