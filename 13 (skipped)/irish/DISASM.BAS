CLS
input "What is the memory offset for this file ";pcofs
input "Do you want the output directed to a file ",a$
opt%=0
if a$="y" then opt%=1:input "File to direct to ",a$:open a$ for output as #2
OPEN command$ FOR RANDOM AS #1 LEN = 1: FIELD #1, 1 AS a$
pc=0
PRINT "Addr.   OpCodes               Assembler"
PRINT "-----   --------------------  ------------------------------"
disasm.more:
	GET #1, pc + 1
	o% = ASC(a$)
	PRINT MID$(STR$(pc+pcofs), 2, 5); TAB(8);:print using "###";o%;
	if opt%=1 then print #2,MID$(STR$(pc+pcofs),2,5);TAB(8);:print #2,using "###";o%;
	pc = pc + 1
	IF o%=203 THEN GOTO cb.decode
	IF o%=221 THEN GOTO ix.decode
	IF o%=237 THEN GOTO ed.decode
	if o%=253 then goto iy.decode
        RESTORE 1000
	FOR f%=0 TO o%: READ d$: NEXT f%
	IF INSTR(d$, "NN") THEN GOTO embword0
	IF INSTR(d$, "N") THEN GOTO embbyte0
	IF INSTR(d$, "DIS") THEN GOTO embdisp0
prdis:
	PRINT TAB(31);ucase$(d$)
	if opt%=1 then PRINT #2,TAB(31);ucase$(d$)
	i$=INKEY$
	if i$="m" then input "New location to disassemble from ",pc
	if eof(1) then close:stop
	if i$="s" then close :stop
	GOTO disasm.more

embword0:
	GET #1, pc + 1: lo = ASC(a$): GET #1, pc + 2: hi = ASC(a$)
	PRINT ",";
	print using "###";lo;
	print ",";
	print using "###";hi;
	if opt%=1 then
		PRINT #2,",";:print #2,using "###";lo;
		print #2,",";:print #2,using "###";hi;
		end if
	eword& = CLNG(hi * 256 + lo)
	t$=MID$(d$, 1, INSTR(d$, "NN") - 1)
	t$=t$+MID$(STR$(eword&), 2, 5)
	t$=t$+MID$(d$, INSTR(d$, "NN") + 2, 10)
	pc = pc + 2
        d$=t$:goto prdis
	GOTO disasm.more

embbyte0:
	GET #1, pc + 1: ebyte = ASC(a$)
	PRINT ",";
	print using "###"; ebyte;
	if opt%=1 then PRINT #2,",";:print #2,using "###";ebyte;
	t$=MID$(d$,1,INSTR(d$,"N")-1)
	t$=t$+MID$(STR$(ebyte), 2, 3)
	t$=t$+MID$(d$,INSTR(d$, "N") + 1, 10)
	pc = pc + 1
	d$=t$:goto prdis
        GOTO disasm.more

embdisp0:
	GET #1, pc + 1: edisp = ASC(a$)
	IF edisp > 127 THEN edisp = edisp - 256
	PRINT ",";:print using "###";ASC(a$);
	if opt%=1 then PRINT #2,",";:print #2,using "###";ASC(a$);
	t$=MID$(d$,1,INSTR(d$,"DIS")-1)
	t$=t$+STR$(edisp)+"   >"+STR$(pc+1+edisp+pcofs)
	pc = pc + 1
	d$=t$:goto prdis
        GOTO disasm.more

cb.decode:
	GET #1, pc + 1
	o% = ASC(a$)
	PRINT ",";:print using "###"; o%;
	if opt%=1 then PRINT #2,",";:print #2,using "###";o%;
	RESTORE 2000
	FOR f% = 0 TO o%: READ d$: NEXT f%
	pc = pc + 1
	goto prdis
        GOTO disasm.more

